{
	"name": "my_utility",
	"displayName": "my_utility",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.copyLineRefarence",
		"onCommand:extension.viewExplorer",
		"onLanguage:markdown"
	],
	"main": "./out/extension.js",
	"contributes": {
		"keybindings": [
			{
				"command": "extension.copyLineRefarence",
				"key": "ctrl+alt+p",
				"mac": "cmd+alt+p",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.viewExplorer",
				"key": "ctrl+alt+o",
				"mac": "cmd+alt+o",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"clipboardy": "^2.1.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {}
}
